{
    "name": "gosuperscript/loqate-php",
    "description": "Loqate API integration with Saloon",
    "type": "library",
    "require": {
        "php": "^8.2",
        "saloonphp/saloon": "^3.0",
        "illuminate/collections": "^10.0"
    },
    "keywords": [
        "saloon",
        "sdk",
        "api",
        "api-integrations",
        "loqate"
    ],
    "authors": [
        {
            "name": "Erik Gaal",
            "email": "erik.gaal@gosuperscript.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Superscript\\Loqate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Superscript\\Loqate\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.24",
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.13"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
